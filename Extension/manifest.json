{
    "name": "Follow",
    "description": "",
    "version": "0.1",
    "permissions": [
        "tabs", "<all_urls>"
    ],
    "background": {
        "scripts": [
            "socket.io.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_title": "Make this page red"
    },
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "css": ["follow.css"],
        "js": [
            "jquery-3.1.1.min.js",
            "content.js"
        ],
        "run_at": "document_end"
    }],
    "web_accessible_resources": ["follow.js"],
    "manifest_version": 2
}